  <!-- 根据模块Id删除对应文件 -->
  <update id="delFile" parameterType="java.lang.Integer">
    update t_file set del_flg = 1
	where id in
	(select u.id from 
		(select r.fid as id from t_file_ref r
		left join t_file f on r.fid = f.id
		where r.refid = #{id,jdbcType=INTEGER}
		and r.del_flg = '0'
		and f.del_flg = '0'
		) as u
	)
  </update>
  
  <!-- 根据文件Id激活对应文件 -->
  <update id="activeFile" parameterType="java.lang.Integer">
